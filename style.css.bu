}

.contentWrapper .sideMenu ul,.connectedSortable li>.padding>.panel>.padding,.welcomeText {
margin-bottom:1em;
}

.contentWrapper .sideMenu ul .indent,.portfolio_block .desc {
margin-left:0;
}
}
/* Tablet ----------- */
@media only screen
and (max-width : 1000px) {

	.controls {
		display: none;
	}

	.headerWrapper {
		height: auto;
		padding-top: 10px;
	}

	.mainMenu {
		position: relative;
		bottom: 0;
		margin: 0;
		padding: 0;
		list-style: none;
		font-size: 120%;
	}

	.contentWrapper .sideMenu {
	  margin: 0;
	  padding: 0;
	  width: 200px;
	  position: relative;
	  padding-top: 5px;
	  z-index: 99;
	  top: 0;
	  left: 0;
	}
	.contentWrapper .sideMenu ul {
	  padding: 0px;
	  margin: 0px;
	  list-style: none;
	  margin-bottom: 1em;
	  margin-left: 5px;
	}
	.contentWrapper .sideMenu ul li {
	  margin: 0;
	  padding: 0;
	  margin-bottom: 0;
	  margin-right: 5px;
	  float: left;
	}
	.contentWrapper .sideMenu ul a {
		background: #dce0ed;
		width: 100%;
		display: block;
		text-decoration: none;
		color: #486b7a;
		font-size: 90%;
		border-top-left-radius: 5px 5px;
		border-top-right-radius: 5px 5px;
		border-bottom-left-radius: 0 0;
	}
	.contentWrapper .sideMenu ul a.selected {
		background: #fff;
		color: #000;
	}
	.contentWrapper .sideMenu ul a > .padding {
		padding: 5px;
	}
	.contentWrapper .sideMenu ul .indent {
		margin-left: 10px;
	}

	.contentWrapper .contentTab .contentPanel {
		position: relative;
		margin-left: 0;
		background: #fff;
		clear: both;
	}
	.contentWrapper .contentTab .contentPanel > .padding {
		padding: 10px;
		overflow: auto;
		min-height: 450px;
	}


}

/* Smartphones ----------- */
@media only screen
and (min-device-width : 320px)
and (max-device-width : 480px) {

	body {
		font-size: 100%;
		background: #fff;
	}

	.wrapper {
		margin: 0;
	}


	/* loading icon */
	.ui-icon-loading {
		background: url(../images/ajax-loader.gif);
		background-size: 92px 92px;
	}

	.ui-loader .ui-icon {
		background-color: #000;
		display: block;
		margin: 0;
		width: 88px;
		height: 88px;
		padding: 1px;
		-webkit-border-radius: 72px;
		-moz-border-radius: 72px;
		border-radius: 72px;
	}


	.mainMenu {
		position: relative;
		font-size: 200%;
	}
	.mainMenu li {
		float: left;
		margin-left: 0;
		width: 33.3%;
	}
	.mainMenu li:last-child {
		margin-right: 0;
	}
	.mainMenu li a {
		display: block;
		padding: 0;
		text-align: center;
		line-height: 2em;
		background: #dce0ed;
		text-decoration: none;
		color: #486b7a;
		font-size: 90%;
	}
	.mainMenu li a.selected {
		background: #486c7c;
		color: #fff;
	}

	.contentWrapper {
		background: #486b7a;
	}

	.contentWrapper .sideMenu {

		width: auto;
		/*
		position: relative;
		padding-top: 0;
		z-index: 0;
		top: 0;
		left: 0;
		min-height: auto;*/
	}
	.contentWrapper .sideMenu ul {
		margin-bottom: 1em;
	}
	.contentWrapper .sideMenu ul li {
		margin-bottom: auto;
		float: left;
		margin-right: 5px;

		margin-right: 5%;
		margin-bottom: 1em;
	}
	.contentWrapper .sideMenu ul a {
		background: #dce0ed;
		width: 100%;
		display: block;
		text-decoration: none;
		color: #486b7a;
		font-size: 150%;
		line-height: 2em;
		padding: 0 10px 10px;
	}
	.contentWrapper .sideMenu ul .indent {
		margin-left: 0;
	}

	.col1, .col2 {
		float: none;
		width: 100%;
	}

	.welcomeText {
		padding: 2%;
		background: #ffa420;
	}

	.connectedSortable li > .padding > .panel > .padding {
		padding: 5%;
	}

	.welcomeText,
	.connectedSortable li > .padding > .panel > .padding {
		margin-bottom: 1em;
	}

	.blog_panel_new .body,
	.blog_panel_old .body {
		height: 500px;
	}
	.blog_floater_old {
		width: 100%;
		float: none;
	}
	.blog_panel {
		margin-bottom: 1em;
	}

	.albumFrame {
		position: relative;
		width: 46%;
		margin-bottom: 1em;
		margin: 0 2% 2%;
		float: left;
	}

	.blog_panel {
		padding-top: 1em;
	}

	.portfolio_block {
	  float: left;
	  margin-bottom: 1em;
	  border-bottom: 1px dotted #ccc;
	  padding-bottom:1em;
	}
	.portfolio_block .image {
	  position: relative;
	  float: left;
	}
	.portfolio_block .image img {
	  width: 400px;
	  height: 400px;
	  margin-right: 20px;
	}
	.portfolio_block .desc {
	  margin-left: 0;
	}

	.social_media_links img {
		width: 30%;
		height: 30%;
		text-align: center;
	}

}
