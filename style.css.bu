/*
Theme Name: Martynbiz14
Theme URI: https://github.com/martynbiz/wp-theme-martynbiz14
Author: Martyn Bissett
Author URI: http://martyn.biz/
Description: Wordpress theme
*/

.ui-icon-loading {
background:url(http://staticfiles.rtyn.biz/martynbiz/images/ajax-loader.gif);
background-size:46px 46px;
}

.ui-loader .ui-icon {
background-color:#000;
display:block;
width:44px;
height:44px;
-webkit-border-radius:36px;
-moz-border-radius:36px;
border-radius:36px;
margin:0;
padding:1px;
}

.ui-content {
padding:0;
}

img.alignleft {
float:left;
margin-right:10px;
}

body {
color:#666;
font-family:"PT Serif","Palatino Linotype","Book Antiqua",Palatino,serif;
font-size:120%;
background:#fff url(http://staticfiles.rtyn.biz/martynbiz/images/bg_mountains_dark.gif) top center no-repeat;
background-attachment:fixed;
margin:0;
padding:0;
}

.welcomeText h2 {
font-weight:700;
font-family:"Lucida Console",Monaco,monospace;
font-size:170%;
margin-bottom:.5em;
color: #ca8119;
opacity:.6;
filter:alpha(opacity=60);
}

h1,h2,h3,h4,p {
margin:0 0 1em;
}

a,a:visited {
color:#486b7a;
}

.controls {
background:#fff url(http://staticfiles.rtyn.biz/martynbiz/images/bg_controls.gif) repeat-x;
position:relative;
height:40px;
padding:5px;
}

.controls h1 {
width:397px;
height:44px;
position:absolute;
top:2px;
left:0;
margin:0;
padding:0;
}

.controls h1 span {
position:absolute;
left:-99999px;
}

.controls .buttons {
position:absolute;
bottom:0;
right:0;
padding:5px;
}

.wrapper {
max-width:1200px;
margin:0 8%;
}

.loading {
margin:auto;
}

.headerWrapper {
position:relative;
height:50px;
}

.mainMenu {
position:absolute;
bottom:0;
list-style:none;
font-size:120%;
margin:0 0 0 207px;
padding:0;
}

.mainMenu li {
float:left;
margin-left:5px;
}

.mainMenu li a {
display:block;
line-height:2em;
background:#dce0ed;
text-decoration:none;
color:#486b7a;
font-size:90%;
border-top-left-radius:5px 5px;
border-top-right-radius:5px 5px;
padding:0 10px;
}

.mainMenu li a:hover,.mainMenu li.current_page_item a {
background:#799fb6;
color:#fff;
box-shadow:0 0 10px #6a9fb6;
}

.contentWrapper,.contentWrapper .contentTab .contentPanel,.welcomeText {
border-radius:5px;
}

.contentWrapper .sideMenu ul a {
border-top-left-radius:5px 5px;
border-bottom-left-radius:5px 5px;
background:#dce0ed;
display:block;
text-decoration:none;
color:#486b7a;
font-size:90%;
padding:5px;
}

.contentWrapper {
width:100%;
float:left;
background:#6a9fb6 url(http://staticfiles.rtyn.biz/martynbiz/images/bg_bluepanel_moreblur.jpg) top center no-repeat;
background-attachment:fixed;
position:relative;
margin-bottom:1em;
box-shadow:0 0 10px #6a9fb6;
padding:10px;
}

.contentWrapper .contentTab {
position:relative;
}

.contentWrapper .sideMenu {
width:200px;
position:absolute;
z-index:99;
top:15px;
left:15px;
margin:0;
padding:5px 0 0;
}

.contentWrapper .sideMenu ul {
list-style:none;
margin:0 0 1em;
padding:0;
}

.contentWrapper .sideMenu ul li {
margin:0 0 5px;
padding:0;
}

.contentWrapper .sideMenu ul a:hover,.contentWrapper .sideMenu ul li.current_page_item a {
background:#fff;
color:#000;
}

.contentWrapper .sideMenu ul .indent {
margin-left:10px;
}

.hr {
border-bottom:1px dotted #ccc;
margin-bottom:1em;
padding-top:1em;
}

.contentWrapper .contentTab .contentPanel {
position:relative;
margin-left:205px;
background:#fff;
}

.contentWrapper .contentTab .contentHome h3 a {
text-decoration:none;
font-size:160%;
}

.contentWrapper .contentTab .contentPanel>.padding {
overflow:auto;
min-height:450px;
position:relative;
padding:20px;
}

.blogContainer .post {
margin-bottom:2em;
}

.blogContainer .post h3 a {
text-decoration:none;
color:#666;
font-weight:700;
}

.blogContainer .post pre {
	background: #ffc;
	padding: 10px;
	border: 1px solid #aa8;
}

.blogContainer .post .pre {
	background: #ffc;
	font-family: courier, Monospace;
}

.blogContainer .post .subheader a.ui-link {
background:#ffa420;
text-decoration:none;
color:#fff;
font-weight:700;
}

.blogContainer .post .subheader a.ui-link:hover {
border-bottom:1px solid #999;
}

.blogContainer .post .post_header {
margin-bottom:.5em;
padding-bottom:.5em;
border-bottom:2px solid #ccc;
}

.blogContainer .post .post_header h3,.blogContainer .post .post_header p {
margin-bottom:.5em;
}

.blogContainer .post .post_header .date {
color:#aaa;
}

.albumFrame {
position:relative;
width:29%;
float:left;
margin:0 2% 2%;
}

.albumFrame .image a {
display:block;
width:100%;
border-top:1px solid #eee;
border-right:1px solid #ccc;
border-bottom:1px solid #ccc;
border-left:1px solid #eee;
background:#fff;
margin-top:10px;
padding:5px;
}

.albumFrame .image img {
width:100%;
height:auto;
}

.albumFrame .title {
position:absolute;
background:#fff;
border:1px solid #ccc;
top:5px;
left:-5px;
transform:rotate(-2deg);
-ms-transform:rotate(-2deg);
-webkit-transform:rotate(-2deg);
padding:5px;
}

.homepanel h3 {
box-shadow:1px 1px 2px rgba(0,0,0,.4);
position:absolute;
background:#fff;
border:1px solid #ccc;
top:-5px;
left:-5px;
transform:rotate(-2deg);
-ms-transform:rotate(-2deg);
-webkit-transform:rotate(-2deg);
padding:5px;
}

.col1 {
float:left;
width:55%;
margin-right:5%;
position:relative;
}

.col2 {
float:left;
width:40%;
position:relative;
}

.welcomeText {
font-size:100%;
color:#fff;
background:#ffa420;
background-size:auto 100%;
margin-bottom:1em;
}

.welcomeText .myname {
color:#905500;
}

.welcomeText p:last-child {
margin-bottom:0;
}

.homepanel {
position:relative;
padding-top:2em;
margin-bottom:2em;
}

.homepanel .date {
color:#999;
}

.sidepanel {
background:#dce0ed;
margin-bottom:1em;
padding:10px;
}

.sidepanel h3 {
font-size:100%;
}

.confirmsent {
text-align:center;
display:none;
}

.blog img {
background:#fff;
border-top:1px solid #eee;
border-right:1px solid #ccc;
border-bottom:1px solid #ccc;
border-left:1px solid #eee;
padding:7px;
}

.blog_panel {
margin-bottom:10px;
position:relative;
padding-top:20px;
}

.blog_panel .body {
position:relative;
overflow:hidden;
margin-bottom:5px;
}

.blog_panel .grad {
position:absolute;
bottom:0;
left:0;
width:100%;
height:50px;
background:url(http://staticfiles.rtyn.biz/martynbiz/images/grad_grey.png) top left repeat-x;
}

.blog_panel .readmore a {
color:#666;
font-weight:700;
text-transform:uppercase;
}

.blog_panel h3 {
position:absolute;
z-index:999999;
background:#fff;
border:1px solid #ccc;
top:0;
left:-5px;
transform:rotate(-2deg);
-ms-transform:rotate(-2deg);
-webkit-transform:rotate(-2deg);
padding:5px;
}

.blog_panel:hover {
cursor:hand;
}

.blog_floater_old {
width:50%;
float:left;
}

.blog_floater_new .blog_floater_padding,.blog_floater_old .blog_floater_padding {
padding-right:10px;
}

.blog_panel_over {
background:#ddd;
}

.blog_panel_out {
background:#eee;
}

.blog_panel_new .body {
height:300px;
}

.blog_panel_old .body {
height:200px;
}

.contactform {
width:80%;
margin:0 auto;
}

#cntctfrm_contact_form input.text,#cntctfrm_contact_form textarea {
width:96%;
outline:0;
border:1px solid #FEFEFE;
border-radius:3px;
background:linear-gradient(top,#EEE0,#FFF100%);
box-shadow:1px 1px 2px rgba(0,0,0,.4);
color:#444;
line-height:1em;
font-size:100%;
margin-bottom:1em;
padding:.6em 2%;
}

#cntctfrm_contact_form label {
display:inline-block;
vertical-align:middle;
text-transform:uppercase;
text-shadow:1px 1px 0 #FFF;
}

#cntctfrm_contact_form input:focus,#cntctfrm_contact_form textarea:focus {
box-shadow:0 0 8px rgba(0,0,0,.4);
}

#cntctfrm_contact_form input[type=submit] {
width:auto;
border-top:1px solid #528ea9;
border-right:1px solid #86b1c4;
border-bottom:1px solid #86b1c4;
border-left:1px solid #528ea9;
border-radius:3px;
background:linear-gradient(top,#6a9fb60,#45788d100%);
box-shadow:2px 2px 2px rgba(0,0,0,.4);
color:#FFF;
text-transform:uppercase;
text-shadow:1px 1px 1px #1c5300;
font-size:1em;
cursor:pointer;
padding:.5em .8em;
}

.portfolio_block {
float:left;
margin-bottom:1em;
border-bottom:1px dotted #ccc;
padding-bottom:1em;
}

.portfolio_block .image {
position:relative;
float:left;
}

.portfolio_block .image img {
background:#fff;
border-top:1px solid #eee;
border-right:1px solid #ccc;
border-bottom:1px solid #ccc;
border-left:1px solid #eee;
width:200px;
height:200px;
padding:7px;
}

.portfolio_block .image h2 {
position:absolute;
background:#fff;
border:1px solid #ccc;
top:-5px;
left:-5px;
transform:rotate(-2deg);
-ms-transform:rotate(-2deg);
-webkit-transform:rotate(-2deg);
padding:5px;
}

.portfolio_block .desc {
margin-left:240px;
}

.social_media_links img {
width:50px;
height:50px;
display:inline;
}

.social_media_links a {
display:inline;
}

.ui-btn-inner,.contactform .submit .ui-btn-inner {
display:none;
}

.welcomeText>.padding,.sidepanel>.padding,.blog_panel .padding {
padding:5px;
}

.blog_panel .readmore,#cntctfrm_contact_form .submit {
text-align:right;
}

@media only screen and max-width 1000px{
.controls {
display:none;
}

.headerWrapper {
height:auto;
padding-top:10px;
}

.mainMenu {
position:relative;
bottom:0;
list-style:none;
font-size:120%;
margin:0;
padding:0;
}

.contentWrapper .sideMenu {
width:200px;
position:relative;
z-index:99;
top:0;
left:0;
margin:0;
padding:5px 0 0;
}

.contentWrapper .sideMenu ul {
list-style:none;
margin:0 0 1em 5px;
padding:0;
}

.contentWrapper .sideMenu ul li {
float:left;
margin:0 5px 0 0;
padding:0;
}

.contentWrapper .sideMenu ul a {
background:#dce0ed;
width:100%;
display:block;
text-decoration:none;
color:#486b7a;
font-size:90%;
border-top-left-radius:5px 5px;
border-top-right-radius:5px 5px;
border-bottom-left-radius:0 0;
}

.contentWrapper .sideMenu ul a.selected {
background:#fff;
color:#000;
}

.contentWrapper .sideMenu ul a>.padding {
padding:5px;
}

.contentWrapper .sideMenu ul .indent {
margin-left:10px;
}

.contentWrapper .contentTab .contentPanel {
position:relative;
margin-left:0;
background:#fff;
clear:both;
}

.contentWrapper .contentTab .contentPanel>.padding {
overflow:auto;
min-height:450px;
padding:10px;
}
}

